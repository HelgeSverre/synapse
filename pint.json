{
    "preset": "laravel",
    "rules": {
        "declare_strict_types": true,
        "final_class": false,
        "void_return": true,
        "ordered_imports": {
            "sort_algorithm": "alpha"
        },
        "no_unused_imports": true,
        "single_quote": true,
        "trailing_comma_in_multiline": {
            "elements": ["arguments", "arrays", "parameters"]
        }
    }
}
